<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
  <color name="text_color_maroon">#800000</color>
  <color name="background_color_light_purple">#e6e6fa</color>
  <color name="text_color_mint">#006666</color>
  <color name="text_color_white">#FFFFFF</color>
  <color name="background_color_blue">#135871</color>
  <color name="custom_theme_color">#268b75</color>
  <color name="actionbar_text">#e9d7d7</color>
  <color name="lightblue">#58D3F7</color>
  <color name="blue">#0040FF</color>
  <color name="darkblue">#0431B4</color>
  <color name="steel_blue">#4682B4</color>
  <color name="pale_gray">#d3d3d3</color>
  <color name="actionbar_background">#32534e</color>
</resources>
